<!DOCTYPE html>
<!--[if IE 7]>                  <html class="ie7 no-js" lang="en">     <![endif]-->
<!--[if lte IE 8]>              <html class="ie8 no-js" lang="en">     <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--> <html class="not-ie no-js" lang="en">  <!--<![endif]-->
<head>
    {% block head %}
        <!-- Basic Page Needs -->
        <meta charset="utf-8">
        <title>{% block title %}Covoiturage {% endblock %}</title>
        <meta name="description" content="">
        <meta name="author" content="">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">

        <!-- Mobile Specific Metas -->
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <!-- Styles -->

        <!-- Bootstrap -->
        <link href="{{ asset('bundles/covevennement/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Forms -->
        <link href="{{ asset('bundles/covevennement/css/jquery.idealforms.css') }}" rel="stylesheet">
        <!-- Select  -->
        <link href="{{ asset('bundles/covevennement/css/jquery.idealselect.css') }}" rel="stylesheet">
        <!-- Slicknav  -->
        <link href="{{ asset('bundles/covevennement/css/slicknav.css') }}" rel="stylesheet">
        <!-- Main style -->
        <link href="{{ asset('bundles/covevennement/css/style.css') }}" rel="stylesheet">

        <!-- Modernizr -->
        <script src="{{ asset('bundles/covevennement/js/modernizr.js') }}"></script>

        <!-- Fonts -->
        <link href="{{ asset('bundles/covevennement/css/font-awesome.min.css') }}" rel="stylesheet">
        <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->


    {% endblock %}
</head>

<body>

<header class="header">

    <div class="top-menu">

        <section class="container">
            <div class="row">

                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="user-log">

                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                            Connect√© en tant que <a title="Voir Profile" href="{{ path('fos_user_profile_show') }}"> {{ app.user.username }}</a>
                            <a title="Voir Profile" href="{{ path('fos_user_profile_show') }}"> <img width="50px" height="50px" src=" {{ asset('uploads/photos/' ~ app.user.photo) }}"></a>
                            -
                            <a href="{{ path('fos_user_security_logout') }}"> Log Out</a>
                        {% else %}
                            <a href="{{ path('fos_user_security_login') }}">Login </a>
                            <a href="{{ path('fos_user_registration_register') }}">Register</a>


                        {% endif %}

                    </div><!-- end .user-log -->
                </div><!-- end .col-sm-4 -->

                <div class="col-md-8 col-sm-8 col-xs-12">

                    <ul class="social-icons">
                        <li>
                            <a class="facebook" href="#">
                                <i class="fa fa-facebook"></i>
                            </a>
                        </li>
                        <li>
                            <a class="twitter" href="#">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li>
                            <a class="google" href="#">
                                <i class="fa fa-google-plus"></i>
                            </a>
                        </li>
                    </ul>

                </div><!-- end .col-sm-8 -->

            </div><!-- end .row -->
        </section><!-- end .container -->

    </div><!-- end .top-menu -->

    <div class="main-baner">

        <div class="background parallax clearfix" style="background-image:url('{{ asset('bundles/covevennement/img/tumblr_n7yhhvUQtx1st5lhmo1_1280.jpg') }}');" data-img-width="1600" data-img-height="1064">

            <div class="main-parallax-content">

                <div class="second-parallax-content">

                    <section class="container">

                        <div class="row">

                            <div class="main-header-container clearfix">

                                <div class="col-md-4 col-sm-12 col-xs-12">

                                    <div class="logo">
                                        <h1>My ride</h1>
                                    </div><!-- end .logo -->

                                </div><!-- end .col-sm-4 -->

                                <div class="col-md-8 col-sm-8 col-xs-12">

                                    <nav id="nav" class="main-navigation">

                                        <ul class="navigation">
                                            <li>
                                                <a href="{{ path('cov_home_homepage') }}">Home</a>
                                            </li>

                                            <li>

                                                <a href="{{ path('cov_annonce_homepage') }}">rides</a>

                                                <ul class="sub-menu">
                                                    <li>
                                                        <a href="{{ path('cov_demande_trajet_homepage') }}">Demande Trajet</a>
                                                    </li>
                                                </ul>

                                            </li>
                                            <li>
                                                <a href="{{ path('cov_reclamation_homepage') }}">Reclamations</a>
                                                <ul class="sub-menu">

                                                    <li>
                                                        <a href="{{ path('cov_temoignage_homepage') }}">Temoignages</a>
                                                    </li>

                                                    <li>
                                                        <a href="{{ path('cov_temoignage_ajout') }}">Temoigner</a>
                                                    </li>
                                                </ul>

                                            </li>
                                            <li>
                                                <a href="{{ path('cov_chat_homepage') }}">Chat Room</a>
                                            </li>
                                            <li>
                                                <a href="{{ path('discutea_forum_homepage') }}">Forum</a>
                                            </li>

                                        </ul>

                                    </nav><!-- end .main-navigation -->

                                </div><!-- end .col-md-8 -->

                            </div><!-- end .main-header-container -->

                        </div><!-- end .row -->

                    </section><!-- end .container -->

                </div><!-- end .second-parallax-content -->

            </div><!-- end .main-parallax-content -->

        </div><!-- end .background .parallax -->

    </div><!-- end .main-baner -->

</header><!-- end .header -->

<section class="main-content">

    {% block container %} {% endblock %}

</section><!-- end .main-content -->

<footer id="footer">

    {% block footer %}

        <div class="footer-copyright">

            <div class="container">
                <div class="row">

                    <div class="col-md-12 col-sm-12 col-xs-12">
                        Copyright by My ride
                    </div>

                </div><!-- end .row -->
            </div><!-- end .container -->

        </div><!-- end .footer-copyright -->
    {% endblock %}
</footer><!-- end #footer -->





{% block javascript %}

    <!-- Javascript -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/bootstrap.min.js') }}"></script>
    <!-- Main jQuery -->
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/jquery.main.js') }}"></script>
    <!-- Form -->
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/jquery.idealforms.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/jquery.idealselect.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/jquery-ui-1.10.4.custom.min.js') }}"></script>
    <!-- Menu -->
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/hoverIntent.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/superfish.js') }}"></script>
    <!-- Counter-Up  -->
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js"></script>
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/jquery.counterup.min.js') }}"></script>
    <!-- Rating  -->
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/bootstrap-rating-input.min.js') }}"></script>
    <!-- Slicknav  -->
    <script type="text/javascript" src="{{ asset('bundles/covevennement/js/jquery.slicknav.min.js') }}"></script>

    <script src="{{ asset('https://cdn.ckeditor.com/4.5.7/standard/ckeditor.js') }}"></script>
    <script>
        $(function () {
            // Replace the <textarea id="editor1"> with a CKEditor
            // instance, using default configuration.
            CKEDITOR.replace('editor1');
            //bootstrap WYSIHTML5 - text editor
            $(".textarea").wysihtml5();
        });
    </script>
{% endblock %}

</body>
</html>